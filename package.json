{
  "name": "@nostr-git/core",
  "version": "1.0.0-alpha.1",
  "packageManager": "pnpm@10.12.4",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js",
      "default": "./dist/events/index.js"
    },
    "./git": {
      "types": "./dist/git/index.d.ts",
      "import": "./dist/git/index.js",
      "default": "./dist/git/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "default": "./dist/types/index.js"
    },
    "./worker": {
      "types": "./dist/worker/index.d.ts",
      "import": "./dist/worker/index.js",
      "default": "./dist/worker/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "default": "./dist/api/index.js"
    },
    "./blossom" : {
      "types": "./dist/blossom/index.d.ts",
      "import": "./dist/blossom/index.js",
      "default": "./dist/blossom/index.js"
    },
    "./stack" : {
      "types": "./dist/stack/index.d.ts",
      "import": "./dist/stack/index.js",
      "default": "./dist/stack/index.js"
    },
    "./utils" : {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "tsconfig.base.json"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc -p tsconfig.base.json",
    "typecheck": "tsc -p tsconfig.base.json --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "prepare": "pnpm build"
  },
  "dependencies": {
    "@isomorphic-git/lightning-fs": "^4.6.2",
    "buffer": "^6.0.3",
    "comlink": "^4.4.2",
    "diff": "^8.0.2",
    "file-type": "^20.0.0",
    "isomorphic-git": "^1.30.1",
    "nostr-tools": "^2.10.0",
    "parse-diff": "^0.11.1",
    "parse-patch": "^0.1.4",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.18.0",
    "fake-indexeddb": "^6.2.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.20.0",
    "vitest": "^3.2.4"
  }
}
